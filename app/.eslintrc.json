{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "max-len": ["error", { "code": 80 }]
    }
}